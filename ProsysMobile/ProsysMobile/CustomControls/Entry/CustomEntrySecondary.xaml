<?xml version="1.0" encoding="utf-8" ?>
<yummy:PancakeView xmlns="http://xamarin.com/schemas/2014/forms"
                   xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                   xmlns:effects="clr-namespace:WiseMobile.Renderer"
                   xmlns:yummy="clr-namespace:Xamarin.Forms.PancakeView;assembly=Xamarin.Forms.PancakeView"
                   x:Class="WiseMobile.CustomControls.Entry.CustomEntrySecondary"
                   Style="{DynamicResource Views.ThreadDepth.LighterBorder}"
                   VerticalOptions="FillAndExpand"
             HorizontalOptions="FillAndExpand"
             x:Name="BorderMain">

    <effects:CustomEntry x:Name="ItemEntry" Style="{DynamicResource Controls.TextFields.EntrySecondary}" TextChanged="OnEntryTextChanged"/>
</yummy:PancakeView>